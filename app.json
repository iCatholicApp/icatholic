{
  "expo": {
    "name": "iCatholic",
    "slug": "iCatholic",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "icatholic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ozdv.icatholic",
      "buildNumber": "9"
    },
    "android": {
      "package": "com.ozdv.icatholic",
      "versionCode": 2,
      "adaptiveIcon": {
        // "foregroundImage": "", not sure what this is
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "cd3338fb-6026-4600-9ea8-01843915d22c"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "sounds": ["./assets/notification.wav"]
        }
      ]
    ]
  }
}
